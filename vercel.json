{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "build": {
    "env": {
      "PIP_NO_CACHE_DIR": "off",
      "PIP_DISABLE_PIP_VERSION_CHECK": "on",
      "PIP_DEFAULT_TIMEOUT": "100"
    }
  }
}
